@model PacotesTuristicos
@{
    ViewData["Title"]="Cadastro de usuario";
}
<h2>Cadastro de usuario</h2>
<form asp-action="Cadastro" method="POST">
    <p>
        <label asp-for="Nome">Nome:</label>
        <input asp-for="Nome">
    </p>
    <p>
        <label asp-for="Origem">Origem:</label>
        <input asp-for="Origem">
    </p>
    <p>
        <label asp-for="Destino">Destino:</label>
        <input asp-for="Destino">
    </p>
   <p>
        <label asp-for="Atrativos">Atrativos:</label>
        <input asp-for="Atrativos">
    </p>
    <p>
        <label asp-for="DataSaida">Data Saida:</label>
        <input type="Date" asp-for="DataSaida">
    </p>
    <p>
        <label asp-for="DataRetorno">Data Retorno:</label>
        <input type="Date" asp-for="DataRetorno">
    </p>
    
    <p>
        <input type="submit" value="Cadastrar">
    </p>

    <p id="Mensagem">
        @ViewBag.Mensagem
    </p>
</form>